      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "highlightedpoints": {
      "type": "blocks"
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
