{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "excerpt": {
      "type": "string"
    },
    "content": {
      "type": "blocks"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "blog": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog.blog"
    },
    "publishedat": {
      "type": "datetime"
    },
    "tags": {
      "type": "json"
    },
    "featured": {
      "type": "boolean"
    }
  }
}
