{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "default": "productname",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "sku": {
      "type": "string",
      "unique": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Enterprise Software",
        "Open-source software",
        "Ohter"
      ]
    },
    "quantity_in_stock": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "detail": {
      "type": "blocks"
    },
    "additional": {
      "type": "blocks"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "product"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    }
  }
}
